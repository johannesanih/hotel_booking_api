{
  "name": "Laravel PHP 8.3",
  "image": "mcr.microsoft.com/devcontainers/php:8.3",
  "features": {
    "ghcr.io/devcontainers/features/mysql:1": {},
    "ghcr.io/devcontainers/features/node:1": {}
  },
  "postCreateCommand": "composer install",
  "forwardPorts": [8000],
  "portsAttributes": {
    "8000": {
      "label": "Laravel",
      "onAutoForward": "openBrowser"
    }
  }
}
